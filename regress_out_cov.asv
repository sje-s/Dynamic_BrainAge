deconf=zeros(size(sfnc,1),size(sfnc,2));
cov=[age,sex,site,age.^2,age.*sex edu inc mar emp t_t];
% cov=[age,sex,site,age.^2,age.*sex];
% cov=[age,sex,site,age.^2];
% cov=[age,age.^2];
% cov=[age];
for j=1:size(sfnc,2)
warning off
X=cov;
y2=sfnc(:,j);
b = regress(y2, [ones(length(X),1) X ]);
Yhat = [ones(length(X),1) X]*b;
res = y2- Yhat;
deconf(:,j)=res;
end
X = deconf;